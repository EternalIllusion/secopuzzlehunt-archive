  "content": "<p></p>\n\n<style>\n    .line-container {\n        max-width: 1400px;\n        margin: auto;\n        padding: 30px;\n        background-color: #f9f9f9;\n        border-radius: 12px;\n    }\n    \n    .line-header {\n        text-align: center;\n        margin-bottom: 30px;\n    }\n    \n    .line-progress {\n        font-size: 1.5rem;\n        color: #333;\n        margin-bottom: 15px;\n    }\n    \n    .line-progress-bar {\n        width: 100%;\n        height: 40px;\n        background-color: #e0e0e0;\n        border-radius: 20px;\n        overflow: hidden;\n        margin-bottom: 20px;\n    }\n    \n    .line-progress-fill {\n        height: 100%;\n        background: linear-gradient(90deg, #4caf50, #45a049);\n        transition: width 0.5s ease;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        color: white;\n        font-weight: bold;\n        font-size: 1.2rem;\n    }\n    \n    .line-meta-notice {\n        padding: 20px;\n        background-color: #fff3cd;\n        border: 3px solid #ffc107;\n        border-radius: 10px;\n        margin-bottom: 25px;\n        text-align: center;\n        font-weight: bold;\n        font-size: 1.2rem;\n    }\n    \n    .line-meta-notice.unlocked {\n        background-color: #d4edda;\n        border-color: #28a745;\n        color: #155724;\n    }\n    \n    .line-levels-grid {\n        display: grid;\n        grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));\n        gap: 25px;\n        margin-bottom: 30px;\n    }\n    \n    .line-level-card {\n        background: white;\n        border: 3px solid #ddd;\n        border-radius: 12px;\n        padding: 20px;\n        cursor: pointer;\n        transition: all 0.3s;\n        position: relative;\n        min-height: 180px;\n    }\n    \n    .line-level-card:hover {\n        transform: translateY(-5px);\n        box-shadow: 0 8px 25px rgba(0,0,0,0.15);\n        border-color: #4caf50;\n    }\n    \n    .line-level-card.completed {\n        border-color: #4caf50;\n        background-color: #f1f8f4;\n    }\n    \n    .line-level-card.locked {\n        opacity: 0.5;\n        cursor: not-allowed;\n        border-color: #ccc;\n    }\n    \n    .line-level-card.locked:hover {\n        transform: none;\n        box-shadow: none;\n    }\n    \n    .line-level-number {\n        position: absolute;\n        top: 15px;\n        right: 15px;\n        background: #555;\n        color: white;\n        width: 45px;\n        height: 45px;\n        border-radius: 50%;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        font-weight: bold;\n        font-size: 1.3rem;\n    }\n    \n    .line-level-card.completed .line-level-number {\n        background: #4caf50;\n    }\n    \n    .line-level-card.meta .line-level-number {\n        background: #ff9800;\n    }\n    \n    .line-level-title {\n        font-size: 1.6rem;\n        font-weight: bold;\n        margin-bottom: 15px;\n        color: #333;\n        padding-right: 60px;\n    }\n    \n    .line-level-status {\n        display: inline-block;\n        padding: 6px 14px;\n        border-radius: 15px;\n        font-size: 1rem;\n        font-weight: bold;\n        margin-bottom: 10px;\n    }\n    \n    .line-level-status.completed {\n        background-color: #4caf50;\n        color: white;\n    }\n    \n    .line-level-status.locked {\n        background-color: #999;\n        color: white;\n    }\n    \n    .line-level-attempts {\n        font-size: 1.1rem;\n        color: #666;\n    }\n    \n    .line-level-answer {\n        margin-top: 8px;\n        padding: 8px 12px;\n        background-color: #e8f5e9;\n        border: 2px solid #4caf50;\n        border-radius: 6px;\n        font-family: monospace;\n        font-size: 1.1rem;\n        font-weight: bold;\n        color: #2e7d32;\n        text-align: center;\n        letter-spacing: 1px;\n        text-transform: uppercase;\n    }\n    \n    .line-detail-modal {\n        display: none;\n        position: fixed;\n        top: 0;\n        left: 0;\n        width: 100%;\n        height: 100%;\n        background-color: rgba(0,0,0,0.7);\n        z-index: 1000;\n        overflow-y: auto;\n    }\n    \n    .line-detail-content {\n        background: white;\n        max-width: 1100px;\n        margin: 50px auto;\n        padding: 40px;\n        border-radius: 15px;\n        position: relative;\n    }\n    \n    .line-detail-close {\n        position: absolute;\n        top: 15px;\n        right: 25px;\n        font-size: 2.5rem;\n        cursor: pointer;\n        color: #999;\n        font-weight: bold;\n    }\n    \n    .line-detail-close:hover {\n        color: #333;\n    }\n    \n    .line-detail-title {\n        font-size: 2.3rem;\n        font-weight: bold;\n        margin-bottom: 20px;\n        color: #333;\n    }\n    \n    .line-detail-description {\n        margin-bottom: 25px;\n        font-size: 1.2rem;\n        color: #555;\n        padding: 20px;\n        background-color: #f0f0f0;\n        border-radius: 10px;\n    }\n    \n    .line-detail-image {\n        width: 100%;\n        max-width: 100%;\n        height: auto;\n        border-radius: 10px;\n        box-shadow: 0 4px 15px rgba(0,0,0,0.2);\n        margin-bottom: 30px;\n    }\n    \n    .line-detail-controls {\n        display: flex;\n        gap: 10px;\n        margin-bottom: 15px;\n    }\n    \n    .line-detail-input {\n        flex-grow: 1;\n        padding: 15px;\n        font-size: 1.2rem;\n        border: 2px solid #ddd;\n        border-radius: 8px;\n    }\n    \n    .line-detail-submit {\n        padding: 15px 40px;\n        font-size: 1.2rem;\n        font-weight: bold;\n        background-color: #4caf50;\n        color: white;\n        border: none;\n        border-radius: 8px;\n        cursor: pointer;\n        transition: background-color 0.3s;\n    }\n    \n    .line-detail-submit:hover {\n        background-color: #45a049;\n    }\n    \n    .line-detail-submit:disabled {\n        background-color: #ccc;\n        cursor: not-allowed;\n    }\n    \n    .line-detail-feedback {\n        min-height: 35px;\n        font-weight: bold;\n        font-size: 1.2rem;\n        padding: 12px;\n        border-radius: 8px;\n        text-align: center;\n        margin-bottom: 15px;\n    }\n    \n    .line-detail-attempts {\n        margin-top: 10px;\n        font-size: 1.1rem;\n        color: #666;\n        text-align: center;\n    }\n    \n    .line-detail-actions {\n        display: flex;\n        gap: 10px;\n        margin-top: 20px;\n    }\n    \n    .line-exchange-btn {\n        flex-grow: 1;\n        padding: 12px 20px;\n        font-size: 1rem;\n        font-weight: bold;\n        background-color: #ff9800;\n        color: white;\n        border: none;\n        border-radius: 8px;\n        cursor: pointer;\n        transition: background-color 0.3s;\n    }\n    \n    .line-exchange-btn:hover {\n        background-color: #f57c00;\n    }\n    \n    .line-exchange-btn:disabled {\n        background-color: #ccc;\n        cursor: not-allowed;\n    }\n    \n    @media (max-width: 768px) {\n        .line-container {\n            padding: 15px;\n        }\n        \n        .line-levels-grid {\n            grid-template-columns: 1fr;\n        }\n        \n        .line-detail-content {\n            margin: 20px;\n            padding: 20px;\n        }\n        \n        .line-detail-title {\n            font-size: 1.8rem;\n        }\n    }\n</style>\n\n<div class=\"line-container\">\n    <div class=\"line-header\">\n        <div class=\"line-progress\">\n            å·²å®Œæˆ: <span id=\"line-solved-count\">0</span> / 5 é¢˜\n        </div>\n        <div class=\"line-progress-bar\">\n            <div class=\"line-progress-fill\" id=\"line-progress-fill\" style=\"width: 0%;\">\n                <span id=\"line-progress-text\">0%</span>\n            </div>\n        </div>\n        <div class=\"line-meta-notice\" id=\"line-meta-notice\">\n            å®Œæˆä»»æ„ 4 é¢˜å³å¯è§£é” Meta å…³å¡\n        </div>\n    </div>\n    \n    <div class=\"line-levels-grid\" id=\"line-levels-grid\">\n        <!-- å…³å¡å¡ç‰‡å°†ç”± JavaScript åŠ¨æ€ç”Ÿæˆ -->\n    </div>\n</div>\n\n<!-- å…³å¡è¯¦æƒ…å¼¹çª— -->\n<div class=\"line-detail-modal\" id=\"line-detail-modal\">\n    <div class=\"line-detail-content\">\n        <span class=\"line-detail-close\" id=\"line-detail-close\">&times;</span>\n        <h2 class=\"line-detail-title\" id=\"line-detail-title\">å…³å¡æ ‡é¢˜</h2>\n        <div class=\"line-detail-description\" id=\"line-detail-description\">å…³å¡æè¿°</div>\n        <img id=\"line-detail-image\" src=\"\" alt=\"Puzzle Image\" class=\"line-detail-image\">\n        \n        <div id=\"line-detail-interaction\" style=\"display: none;\">\n            <div class=\"line-detail-controls\">\n                <input type=\"text\" id=\"line-detail-input\" class=\"line-detail-input\" placeholder=\"è¾“å…¥ç­”æ¡ˆ\">\n                <button id=\"line-detail-submit\" class=\"line-detail-submit btn\">{% translate \"æäº¤\" %}</button>\n            </div>\n            <div class=\"line-detail-feedback\" id=\"line-detail-feedback\"></div>\n            <div class=\"line-detail-attempts\" id=\"line-detail-attempts\"></div>\n            <div class=\"line-detail-actions\">\n                <button id=\"line-exchange-btn\" class=\"line-exchange-btn\">\n                    ä½¿ç”¨æç¤ºç‚¹æ¢æ¬¡æ•° (æ¶ˆè€— 1)\n                    (å¯ç”¨: <span id=\"line-hint-balance-display\">{{ team.num_hints_remaining|default:0 }}</span>)\n                </button>\n            </div>\n        </div>\n    </div>\n</div>\n\n<script>\ndocument.addEventListener('DOMContentLoaded', () => {\n    const apiUrl = \"{% url 'line_api' %}\";\n    const csrfToken = \"{{ csrf_token }}\";\n    \n    const levelsGrid = document.getElementById('line-levels-grid');\n    const solvedCountEl = document.getElementById('line-solved-count');\n    const progressFill = document.getElementById('line-progress-fill');\n    const progressText = document.getElementById('line-progress-text');\n    const metaNotice = document.getElementById('line-meta-notice');\n    \n    const modal = document.getElementById('line-detail-modal');\n    const modalTitle = document.getElementById('line-detail-title');\n    const modalDescription = document.getElementById('line-detail-description');\n    const modalImage = document.getElementById('line-detail-image');\n    const modalInteraction = document.getElementById('line-detail-interaction');\n    const modalInput = document.getElementById('line-detail-input');\n    const modalSubmit = document.getElementById('line-detail-submit');\n    const modalFeedback = document.getElementById('line-detail-feedback');\n    const modalAttempts = document.getElementById('line-detail-attempts');\n    const modalClose = document.getElementById('line-detail-close');\n    const exchangeBtn = document.getElementById('line-exchange-btn');\n    const hintBalanceDisplay = document.getElementById('line-hint-balance-display');\n    \n    let currentLevel = null;\n    let levelsData = [];\n    \n    function formatHintBalance(val) {\n        const num = Number(val);\n        if (Number.isNaN(num)) return val;\n        const truncated = Math.trunc(num * 100) / 100;\n        return truncated.toFixed(2);\n    }\n    \n    // API è°ƒç”¨\n    async function apiCall(data) {\n        try {\n            const response = await fetch(apiUrl, {\n                method: 'POST',\n                headers: {\n                    'Content-Type': 'application/json',\n                    'X-CSRFToken': csrfToken\n                },\n                body: JSON.stringify(data)\n            });\n            \n            if (!response.ok) {\n                const errorData = await response.json();\n                return { success: false, error: errorData.message || `HTTP error! status: ${response.status}` };\n            }\n            return response.json();\n        } catch (error) {\n            console.error('API call failed:', error);\n            return { success: false, error: 'Network error' };\n        }\n    }\n    \n    // åŠ è½½æ‰€æœ‰å…³å¡\n    async function loadAllLevels() {\n        const response = await apiCall({ action: 'get_all_levels' });\n        \n        if (response.success) {\n            levelsData = response.levels;\n            solvedCountEl.innerText = response.solved_count;\n            \n            const progress = Math.round((response.solved_count / 5) * 100);\n            progressFill.style.width = progress + '%';\n            progressText.innerText = progress + '%';\n            \n            if (response.meta_unlocked) {\n                metaNotice.className = 'line-meta-notice unlocked';\n                metaNotice.innerText = 'âœ“ Meta å…³å¡å·²è§£é”ï¼';\n            }\n            \n            renderLevels();\n        } else {\n            levelsGrid.innerHTML = '<p style=\"color: red;\">åŠ è½½å¤±è´¥: ' + response.error + '</p>';\n        }\n    }\n    \n    // æ¸²æŸ“å…³å¡å¡ç‰‡\n    function renderLevels() {\n        levelsGrid.innerHTML = '';\n        \n        levelsData.forEach(level => {\n            // Metaå…³å¡æœªè§£é”æ—¶ä¸æ˜¾ç¤º\n            if (level.level === 6 && !level.accessible) {\n                return;\n            }\n            \n            const card = document.createElement('div');\n            card.className = 'line-level-card';\n            \n            if (level.completed) {\n                card.classList.add('completed');\n            }\n            if (!level.accessible) {\n                card.classList.add('locked');\n            }\n            if (level.level === 6) {\n                card.classList.add('meta');\n            }\n            \n            let statusHTML = '';\n            if (level.completed) {\n                statusHTML = '<span class=\"line-level-status completed\">âœ“ å·²å®Œæˆ</span>';\n            } else if (!level.accessible) {\n                statusHTML = '<span class=\"line-level-status locked\">ğŸ”’ æœªè§£é”</span>';\n            }\n            \n            let attemptsHTML = '';\n            if (level.submittable && level.accessible && !level.completed) {\n                attemptsHTML = `<div class=\"line-level-attempts\">å‰©ä½™æ¬¡æ•°: ${level.attempts_left}</div>`;\n            }\n            \n            let answerHTML = '';\n            if (level.completed && level.answer) {\n                answerHTML = `<div class=\"line-level-answer\">ç­”æ¡ˆ: ${level.answer}</div>`;\n            }\n            \n            card.innerHTML = `\n                <div class=\"line-level-number\">${level.level}</div>\n                <div class=\"line-level-title\">${level.title}</div>\n                ${statusHTML}\n                ${attemptsHTML}\n                ${answerHTML}\n            `;\n            \n            if (level.accessible) {\n                card.addEventListener('click', () => openLevel(level.level));\n            }\n            \n            levelsGrid.appendChild(card);\n        });\n    }\n    \n    // æ‰“å¼€å…³å¡è¯¦æƒ…\n    function openLevel(levelNum) {\n        const level = levelsData.find(l => l.level === levelNum);\n        if (!level || !level.accessible) return;\n        \n        currentLevel = levelNum;\n        modalTitle.innerText = level.title;\n        modalDescription.innerHTML = level.description;\n        modalImage.src = level.image_url;\n        modalFeedback.innerText = '';\n        modalFeedback.style.backgroundColor = '';\n        modalFeedback.style.color = '';\n        modalInput.value = '';\n        \n        // Metaå…³å¡ä¸æ˜¾ç¤ºè¾“å…¥æ¡†\n        if (level.level === 6) {\n            modalInteraction.style.display = 'none';\n        } else if (level.submittable && !level.completed) {\n            modalInteraction.style.display = 'block';\n            modalAttempts.innerText = `å‰©ä½™æäº¤æ¬¡æ•°: ${level.attempts_left}`;\n            modalInput.disabled = false;\n            modalSubmit.disabled = false;\n        } else if (level.completed) {\n            modalInteraction.style.display = 'block';\n            modalFeedback.innerText = 'âœ“ æœ¬å…³å·²å®Œæˆ';\n            modalFeedback.style.backgroundColor = '#d4edda';\n            modalFeedback.style.color = '#155724';\n            modalInput.disabled = true;\n            modalSubmit.disabled = true;\n            modalAttempts.innerText = '';\n        }\n        \n        modal.style.display = 'block';\n    }\n    \n    // å…³é—­å¼¹çª—\n    function closeModal() {\n        modal.style.display = 'none';\n        currentLevel = null;\n    }\n    \n    modalClose.addEventListener('click', closeModal);\n    modal.addEventListener('click', (e) => {\n        if (e.target === modal) closeModal();\n    });\n    \n    // æäº¤ç­”æ¡ˆ\n    async function handleSubmit() {\n        const submission = modalInput.value.trim();\n        if (!submission || !currentLevel) return;\n        \n        modalFeedback.innerText = 'æ­£åœ¨æäº¤...';\n        modalFeedback.style.backgroundColor = '#f0f0f0';\n        modalFeedback.style.color = '#333';\n        modalSubmit.disabled = true;\n        \n        const response = await apiCall({\n            action: 'submit',\n            level: currentLevel,\n            submission: submission\n        });\n        \n        modalSubmit.disabled = false;\n        \n        if (!response.success) {\n            modalFeedback.innerText = response.message || 'æäº¤å¤±è´¥ï¼Œè¯·é‡è¯•';\n            modalFeedback.style.backgroundColor = '#f8d7da';\n            modalFeedback.style.color = '#721c24';\n            return;\n        }\n        \n        switch (response.result) {\n            case 'correct':\n                modalFeedback.innerText = 'âœ“ æ­£ç¡®ï¼';\n                modalFeedback.style.backgroundColor = '#d4edda';\n                modalFeedback.style.color = '#155724';\n                modalInput.disabled = true;\n                modalSubmit.disabled = true;\n                \n                setTimeout(() => {\n                    closeModal();\n                    loadAllLevels();\n                }, 1500);\n                break;\n                \n            case 'incorrect':\n                modalFeedback.innerText = 'âœ— ç­”æ¡ˆé”™è¯¯';\n                modalFeedback.style.backgroundColor = '#f8d7da';\n                modalFeedback.style.color = '#721c24';\n                modalInput.value = '';\n                modalAttempts.innerText = `å‰©ä½™æäº¤æ¬¡æ•°: ${response.attempts_left}`;\n                \n                if (response.attempts_left <= 0) {\n                    modalInput.disabled = true;\n                    modalSubmit.disabled = true;\n                    modalFeedback.innerText = 'âœ— æœ¬å…³æäº¤æ¬¡æ•°å·²ç”¨å®Œ';\n                }\n                break;\n                \n            case 'already_completed':\n                modalFeedback.innerText = 'âœ“ æœ¬å…³å·²å®Œæˆ';\n                modalFeedback.style.backgroundColor = '#d4edda';\n                modalFeedback.style.color = '#155724';\n                modalInput.disabled = true;\n                modalSubmit.disabled = true;\n                break;\n        }\n    }\n    \n    modalSubmit.addEventListener('click', handleSubmit);\n    modalInput.addEventListener('keyup', (e) => {\n        if (e.key === 'Enter') handleSubmit();\n    });\n    \n    // æç¤ºç‚¹å…‘æ¢\n    async function handleExchange() {\n        if (!currentLevel) return;\n        \n        const cost = 1;\n        if (!confirm(`ä½ ç¡®å®šè¦æ¶ˆè€— ${cost} ä¸ªæç¤ºç‚¹æ¥æ¢å– 3 æ¬¡æäº¤æœºä¼šå—ï¼Ÿ`)) {\n            return;\n        }\n        \n        exchangeBtn.disabled = true;\n        modalFeedback.innerText = 'æ­£åœ¨å…‘æ¢...';\n        modalFeedback.style.backgroundColor = '#f0f0f0';\n        modalFeedback.style.color = '#333';\n        \n        const response = await apiCall({\n            action: 'exchange_hint_for_attempts',\n            level: currentLevel\n        });\n        \n        exchangeBtn.disabled = false;\n        \n        if (response.success) {\n            modalFeedback.innerText = response.message;\n            modalFeedback.style.backgroundColor = '#d4edda';\n            modalFeedback.style.color = '#155724';\n            modalAttempts.innerText = `å‰©ä½™æäº¤æ¬¡æ•°: ${response.new_attempts_left}`;\n            hintBalanceDisplay.innerText = formatHintBalance(response.new_hints_remaining);\n            \n            // æ›´æ–°levelsDataä¸­çš„attempts_left\n            const levelData = levelsData.find(l => l.level === currentLevel);\n            if (levelData) {\n                levelData.attempts_left = response.new_attempts_left;\n            }\n        } else {\n            modalFeedback.innerText = response.message || 'å…‘æ¢å¤±è´¥ï¼';\n            modalFeedback.style.backgroundColor = '#f8d7da';\n            modalFeedback.style.color = '#721c24';\n        }\n    }\n    \n    exchangeBtn.addEventListener('click', handleExchange);\n    \n    // åˆå§‹åŒ–\n    loadAllLevels();\n});\n</script>\n\n<p style=\"text-align: center; margin-top: 30px;\"><a href=\"/DMs\" class=\"btn\">è¿›å…¥ç«™å†…ä¿¡</a></p>\n  ",
