alt=\"6\"><div class=\"mj-tile-answer\"></div></div>\n\t\t<div class=\"mj-tile\" data-tile-id=\"25\"><img src=\"\" alt=\"7\"><div class=\"mj-tile-answer\"></div></div>\n\t\t<div class=\"mj-tile\" data-tile-id=\"26\"><img src=\"\" alt=\"8\"><div class=\"mj-tile-answer\"></div></div>\n\t\t<div class=\"mj-tile\" data-tile-id=\"27\"><img src=\"\" alt=\"9\"><div class=\"mj-tile-answer\"></div></div>\n        <div class=\"mj-divider\"></div>\n\n\t\t<div class=\"mj-tile\" data-tile-id=\"28\"><img src=\"\" alt=\"ä¸œ\"><div class=\"mj-tile-answer\"></div></div>\n\t\t<div class=\"mj-tile\" data-tile-id=\"29\"><img src=\"\" alt=\"å—\"><div class=\"mj-tile-answer\"></div></div>\n\t\t<div class=\"mj-tile\" data-tile-id=\"30\"><img src=\"\" alt=\"è¥¿\"><div class=\"mj-tile-answer\"></div></div>\n\t\t<div class=\"mj-tile\" data-tile-id=\"31\"><img src=\"\" alt=\"åŒ—\"><div class=\"mj-tile-answer\"></div></div>\n\t\t<div class=\"mj-tile\" data-tile-id=\"32\"><img src=\"\" alt=\"ä¸­\"><div class=\"mj-tile-answer\"></div></div>\n\t\t<div class=\"mj-tile\" data-tile-id=\"33\"><img src=\"\" alt=\"å‘\"><div class=\"mj-tile-answer\"></div></div>\n\t\t<div class=\"mj-tile\" data-tile-id=\"34\"><img src=\"\" alt=\"ç™½\"><div class=\"mj-tile-answer\"></div></div>\n\t</div>\n\n\n<!-- Modal -->\n<div class=\"mj-modal-overlay\" id=\"mj-modal\">\n\t<div class=\"mj-modal-content\" id=\"mj-modal-content\">\n\t\t<button class=\"mj-modal-close\" id=\"mj-modal-close\">&times;</button>\n\t\t\n\t\t<div class=\"mj-modal-image\">\n\t\t\t<img id=\"mj-modal-img\" src=\"\" alt=\"\">\n\t\t</div>\n\t\t\n\t\t<div class=\"mj-modal-info\">\n\t\t\t<p><strong>çŠ¶æ€ï¼š</strong><span id=\"mj-tile-status\"></span></p>\n\t\t\t<p id=\"mj-cooldown-info\" style=\"display:none;\"><strong>å†·å´æ—¶é—´ï¼š</strong><span id=\"mj-cooldown-time\">0</span> ç§’</p>\n\t\t\t<p id=\"mj-answer-display\" style=\"display:none;\"><strong>ç­”æ¡ˆï¼š</strong><span id=\"mj-tile-answer\" style=\"color: #4CAF50; font-size: 1.2rem;\"></span></p>\n\t\t</div>\n\t\t\n\t\t<div id=\"mj-interactive-section\">\n\t\t\t<div class=\"mj-exchange-buttons\">\n\t\t\t\t<button id=\"mj-exchange-answer\" class=\"btn\">ç›´æ¥å…‘æ¢ç­”æ¡ˆ</button>\n\t\t\t</div>\n\t\t\t\n\t\t\t<div class=\"mj-modal-submit\">\n\t\t\t\t<input type=\"text\" id=\"mj-answer-input\" placeholder=\"è¾“å…¥ç­”æ¡ˆ\">\n\t\t\t\t<button id=\"mj-submit-btn\" class=\"btn\">æäº¤ç­”æ¡ˆ</button>\n\t\t\t\t<p class=\"mj-modal-feedback\" id=\"mj-feedback\"></p>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</div>\n\n<script>\ndocument.addEventListener('DOMContentLoaded', () => {\n\tconst tiles = document.querySelectorAll('.mj-tile');\n\tconst modal = document.getElementById('mj-modal');\n\tconst modalContent = document.getElementById('mj-modal-content');\n\tconst closeBtn = document.getElementById('mj-modal-close');\n\tconst modalImg = document.getElementById('mj-modal-img');\n\tconst tileStatus = document.getElementById('mj-tile-status');\n\tconst cooldownInfo = document.getElementById('mj-cooldown-info');\n\tconst cooldownTime = document.getElementById('mj-cooldown-time');\n\tconst answerDisplay = document.getElementById('mj-answer-display');\n\tconst tileAnswer = document.getElementById('mj-tile-answer');\n\tconst interactiveSection = document.getElementById('mj-interactive-section');\n\tconst answerInput = document.getElementById('mj-answer-input');\n\tconst submitBtn = document.getElementById('mj-submit-btn');\n\tconst feedback = document.getElementById('mj-feedback');\n\tconst exchangeAnswerBtn = document.getElementById('mj-exchange-answer');\n\tconst hintBalance = document.getElementById('hint-balance');\n\t\n\tconst apiUrl = \"{% url 'majiang_api' %}\";\n\tconst csrfToken = \"{{ csrf_token }}\";\n\tlet currentTileId = null;\n\tlet cooldownTimer = null;\n\n\t// åŠ è½½çŠ¶æ€\n\tasync function loadState() {\n\t\tconst result = await apiCall({ action: 'get_state' });\n\t\tif (result.success) {\n\t\t\tupdateTilesDisplay(result.tiles_status, result.unlocked_tiles);\n\t\t\thintBalance.textContent = result.hint_balance.toFixed(2);\n\t\t}\n\t}\n\n\t// æ›´æ–°éº»å°†ç‰Œæ˜¾ç¤º\n\tfunction updateTilesDisplay(tilesStatus, unlockedTiles) {\n\t\tconst unlockedSet = new Set(unlockedTiles);\n\t\ttiles.forEach(tile => {\n\t\t\tconst tileId = parseInt(tile.dataset.tileId);\n\t\t\tconst tileData = tilesStatus[tileId];\n\t\t\tconst img = tile.querySelector('img');\n\t\t\tconst answerDiv = tile.querySelector('.mj-tile-answer');\n\t\t\t\n\t\t\ttile.classList.remove('locked', 'solved');\n\t\t\t\n\t\t\tif (!unlockedSet.has(tileId)) {\n\t\t\t\t// æœªè§£é”ï¼šä¸æ˜¾ç¤ºå›¾ç‰‡\n\t\t\t\ttile.classList.add('locked');\n\t\t\t\timg.src = '';\n\t\t\t\tif (answerDiv) answerDiv.textContent = '';\n\t\t\t} else if (tileData) {\n\t\t\t\t// å·²è§£é”ï¼šæ˜¾ç¤ºå›¾ç‰‡\n\t\t\t\timg.src = tileData.image;\n\t\t\t\tif (tileData.solved) {\n\t\t\t\t\ttile.classList.add('solved');\n\t\t\t\t\t// æ˜¾ç¤ºç­”æ¡ˆå åŠ å±‚\n\t\t\t\t\tif (answerDiv && tileData.answer) {\n\t\t\t\t\t\tconst answer = tileData.answer;\n\t\t\t\t\t\t// å››å­—ç­”æ¡ˆæ’æˆ2x2\n\t\t\t\t\t\tif (answer.length === 4) {\n\t\t\t\t\t\t\tanswerDiv.innerHTML = answer.substring(0, 2) + '<br>' + answer.substring(2);\n\t\t\t\t\t\t} else {\n\t\t\t\t\t\t\tanswerDiv.textContent = answer;\n\t\t\t\t\t\t}\n\t\t\t\t\t}\n\t\t\t\t} else {\n\t\t\t\t\tif (answerDiv) answerDiv.textContent = '';\n\t\t\t\t}\n\t\t\t}\n\t\t});\n\t}\n\n\t// APIè°ƒç”¨\n\tasync function apiCall(data) {\n\t\tconsole.log('APIè°ƒç”¨:', data);\n\t\ttry {\n\t\t\tconst response = await fetch(apiUrl, {\n\t\t\t\tmethod: 'POST',\n\t\t\t\theaders: {\n\t\t\t\t\t'Content-Type': 'application/json',\n\t\t\t\t\t'X-CSRFToken': csrfToken\n\t\t\t\t},\n\t\t\t\tbody: JSON.stringify(data)\n\t\t\t});\n\t\t\tconst result = await response.json();\n\t\t\tconsole.log('APIå“åº”:', result);\n\t\t\treturn result;\n\t\t} catch (error) {\n\t\t\tconsole.error('API call failed:', error);\n\t\t\treturn { success: false, error: 'Network error' };\n\t\t}\n\t}\n\n\t// æ‰“å¼€æ¨¡æ€æ¡†\n\tasync functio